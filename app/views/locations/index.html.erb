<h1>Location</h1>
<ul>
  <% @locations.each do |location| %>
    <li>
      <a href="/locations/<%=location.id %>">
        <%= location.name %>
      </a>
    </li>
  <% end %>
</ul>

<% if user_signed_in? %>
  <%= current_user.email %>
<% end %>
<p>Find me in app/views/locations/index.html.erb</p>
